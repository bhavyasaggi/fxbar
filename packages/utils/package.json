{
  "name": "@fxbar/utils",
  "version": "0.0.0",
  "description": "Utility functions for fxbar packages",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhavyasaggi/fxbar.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "import": "./dist/*/index.js"
      },
      "require": {
        "types": "./dist/*/index.d.cts",
        "require": "./dist/*/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint:eslint": "eslint . --fix --ext .js,.ts",
    "lint:prettier": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "test": "jest --passWithNoTests",
    "build": "tsup",
    "prepublishOnly": "yarn install --frozen-lockfile && yarn clean && yarn lint && yarn test && yarn build",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": []
}
