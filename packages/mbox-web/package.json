{
  "name": "@fxbar/mbox-web",
  "version": "0.0.0",
  "description": "Mbox parser for client",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhavyasaggi/fxbar.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "import": "./dist/*/index.js"
      },
      "require": {
        "types": "./dist/*/index.d.cts",
        "require": "./dist/*/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "lint": "eslint . --fix --ext .js,.ts",
    "test": "jest --passWithNoTests",
    "build": "tsup",
    "prepublishOnly": "yarn install --frozen-lockfile && yarn clean && yarn lint && yarn test && yarn build",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "postal-mime": "^2.4.3"
  },
  "devDependencies": {
    "@lib/repo-config": "*",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": []
}
